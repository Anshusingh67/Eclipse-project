
public class printing9Printing {
	public static void main(String[] erg) {
		int a =14;
		System.out.printf("%d%n",a);
		System.out.printf("%o%n",a);
		System.out.printf("%X%n",a);
		System.out.printf("%x%n",a);
		System.out.printf("%10d Hi%n",a);
		System.out.printf("%-10 Hi%n",a);
		double b= 12.456;
		System.out.printf("%f%n",b);
		System.out.printf("%.2f%n",b);
		System.out.printf("%e%n",b);
		System.out.printf("%E%n",b);
		boolean c= true;
		System.out.printf("%b%n",c);
		System.out.printf("%B%n",c);
		char d= 'a';
		System.out.printf("%c%n",d);
		System.out.printf("%C%n",d);
		String e= "Incapp";
		System.out.printf("%s%n",e);
		System.out.printf("%S%n",e);
		System.out.printf("%.2S%n",e);
		System.out.printf("%10s%n",e);
		System.out.printf("%-10s%n",e);
		
		
		

		
	}

}
